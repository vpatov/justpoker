{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "max-line-length": [true, 80],
        "one-variable-per-declaration": [false],
        "no-empty": [true, "allow-empty-catch", "allow-empty-functions"],
        "no-switch-case-fall-through": {"severity":"error"}
    },
    "rulesDirectory": []
}
